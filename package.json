  },
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eeditiones/tei-publisher-eleventy-starter.git"
  },
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@teipublisher/pb-components": "^2.12.6",
    "@teipublisher/pb-eleventy-plugin": "^0.10.0",
    "rimraf": "^5.0.5"
  }
}
